#include <stdio.h>

#define SIZE 5

int main() {
    int arr[SIZE] = {1, 2, -1, 4, 5}; // -1 indicates a missing element

    for (int i = 0; i < SIZE; i++) {
        if (arr[i] == -1) {
            printf("Element at index %d is missing.\n", i);
        } else {
            printf("Element at index %d: %d\n", i, arr[i]);
        }
    }
    return 0;
}
